{
  "name": "ce.js",
  "version": "2021.1-beta",
  "description": "Compiler Explorer API client for NodeJS",
  "main": "dist/node.js",
  "types": "dist/node.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supergrecko/ce.js.git"
  },
  "author": "me@supergrecko.com",
  "bugs": {
    "url": "https://github.com/supergrecko/ce.js/issues"
  },
  "homepage": "https://github.com/supergrecko/ce.js#readme",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublishOnly": "yarn compile",
    "compile:node": "yarn fmt && tsc --project tsconfig.json",
    "compile:browser": "yarn compile:node && webpack ./dist/browser.js -o ./browser --mode production",
    "fmt": "prettier --config .prettierrc src/ --write",
    "test": "jest"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.11",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "keywords": [
    "compiler-explorer",
    "compiler"
  ]
}
